   			<!-- specify UTF-8, ISO-8859-1 or any other file encoding -->
   			<encoding>UTF-8</encoding>
   			
   			<tasks>     
        		<echo message="Copying jsps in Web......:${basedir} ,dest:${JSP_PATH_TO_WEB} , src:${JSP_PATH_FROM_WEB}"/> 
         		<copy todir="${JSP_PATH_TO_WEB}" overwrite="true"><!-- destination -->    
          		<fileset dir="${JSP_PATH_FROM_WEB}" includes="*.jsp" /> <!-- source -->   
          		</copy>  
          		   
          		<echo message="Copying resources in Web....:${basedir} ,dest:${JSP_PATH_PROPERTIES_DEST_WEB} , src:${JSP_PATH_PROPERTIES_WEB}"/> 
			    <copy todir="${JSP_PATH_PROPERTIES_DEST_WEB}" overwrite="true">        
